<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <sectionGroup name="sqCopyResolutionGroup">
      <section
        name="sqCopyResolution"
        type="SqCopyResolution.Model.SqCopyResolutionSection, SqCopyResolution"
        allowLocation="true"
        allowDefinition="Everywhere"
      />
    </sectionGroup>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
  </configSections>  
  <sqCopyResolutionGroup>
    <sqCopyResolution>
      <profiles>
        <profile name="Default">
          <sonarQube
            url=""
            userName=""
            password="" />
          <source 
            projectKey="N-central:dev-vNext" />
          <destination 
            projectKeys="N-central:NCPM-PatchStatusServiceMonitoring" />
          <operation 
            type="SyncSubBranches" />
        </profile>
      </profiles>
    </sqCopyResolution>
  </sqCopyResolutionGroup>
  <log4net>
    <appender name="ConsoleAppender" type="log4net.Appender.ConsoleAppender" >
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %-5level %logger - %message%newline" />
      </layout>
    </appender>
    <appender name="RollingFileAppender" type="log4net.Appender.RollingFileAppender">
      <file value="SqCopyResolution.log" />
      <appendToFile value="true" />
      <rollingStyle value="Size" />
      <maxSizeRollBackups value="10" />
      <maximumFileSize value="1MB" />
      <staticLogFileName value="true" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %-5level %logger - %message%newline" />
      </layout>
    </appender>
    <root>
      <level value="INFO" />
      <appender-ref ref="ConsoleAppender" />
      <appender-ref ref="RollingFileAppender" />
    </root>
  </log4net>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
  </startup>
</configuration>